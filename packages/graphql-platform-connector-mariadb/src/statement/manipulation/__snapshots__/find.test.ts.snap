              \`article_tags>tag\`.\`id\` IS NOT NULL,
              JSON_OBJECT(
                "title",
                \`article_tags>tag\`.\`title\`,
                "deprecated",
                \`article_tags>tag\`.\`deprecated\`
              ),
              NULL
            )
          )
          ORDER BY \`article_tags\`.\`order\` ASC
          LIMIT 5
        )
      FROM \`article_tags\`
        INNER JOIN \`tags\` AS \`article_tags>tag\` ON \`article_tags\`.\`tag_id\` <=> \`article_tags>tag\`.\`id\`
      WHERE \`articles\`.\`private_id\` <=> \`article_tags\`.\`article_private_id\`
    ),
    "filteredTags",
