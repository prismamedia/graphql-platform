exports[`Find statement 4 - generates statements 1`] = `
[
  "SELECT JSON_OBJECT(
    "title",
    \`categories\`.\`title\`,
    "childCount",
    (
      SELECT COUNT(*)
      FROM \`categories\` AS \`categories>children\`
      WHERE \`categories\`.\`private_id\` <=> \`categories>children\`.\`parent_private_id\`
    ),
    "children",
    (
      SELECT JSON_ARRAYAGG(
          JSON_OBJECT(
            "parent",
            (
              SELECT JSON_OBJECT("title", \`categories>children>parent\`.\`title\`)
              FROM \`categories\` AS \`categories>children>parent\`
              WHERE \`categories>children\`.\`parent_private_id\` <=> \`categories>children>parent\`.\`private_id\`
              LIMIT 1
            ), "title", \`categories>children\`.\`title\`
          )
          LIMIT 10
        )
      FROM \`categories\` AS \`categories>children\`
      WHERE \`categories\`.\`private_id\` <=> \`categories>children\`.\`parent_private_id\`
    )
  ) as \`Category\`
FROM \`categories\`
WHERE NOT EXISTS (
    SELECT *
    FROM \`categories\` AS \`categories>parent\`
    WHERE \`categories\`.\`parent_private_id\` <=> \`categories>parent\`.\`private_id\`
    LIMIT 1
  )
LIMIT 1",
]
`;
